<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane fx:id="flowPane" xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.maker.foldermaker.controller.ViewController"
          prefHeight="320.0" prefWidth="240.0">
    <VBox fx:id="vBoxContainer">
        <TextField fx:id="textFieldNumberOfGroup" promptText="Number of lab" alignment="CENTER"/>
        <CheckBox fx:id="checkBoxDownloadFiles" text="Download Files"/>
        <Button fx:id="buttonDone" text="Done" onAction="#onDone"/>
    </VBox>
</FlowPane>
